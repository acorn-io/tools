Name: Google Gmail OAuth Credential
Share Credential: ../../../oauth2 as google.gmail.write
    with GMAIL_GOOGLE_OAUTH_TOKEN as env and
        google-gmail as integration and
            "https://www.googleapis.com/auth/userinfo.email
            openid
            https://www.googleapis.com/auth/userinfo.profile
            https://mail.google.com/
            https://www.googleapis.com/auth/gmail.readonly
            https://www.googleapis.com/auth/gmail.compose" as scope
Type: credential

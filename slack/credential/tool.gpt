Name: Slack OAuth Credential
Share Credential: ../../oauth2 as slack.write
    with SLACK_TOKEN as env and
        slack as integration and
        "channels:history
        groups:history
        im:history
        mpim:history
        channels:read
        files:read
        im:read
        search:read
        team:read
        users:read
        groups:read
        chat:write
        groups:write
        mpim:write
        im:write" as scope
Type: credential
